<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    {% block meta %}
    <meta name="author" content="Ada Young" />
    <meta name="description" content="Ada's pretty website!" />
    {% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="//{{ static_domain }}/static/favicon.ico" />

    <title>
      {% block title %} Hello, world! - ada-young.com {% endblock %}
    </title>

    <!-- Bootstrap core CSS -->
    <link
      href="//{{ static_domain }}/static/vendor/css/bootstrap.min.css.br"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="//{{ static_domain }}/static/vendor/css/cookieconsent.min.css.br"
    />
    <link rel="stylesheet" href="//{{ static_domain }}/static/css/base.css" />
    {% block morehead %} {% endblock %}
  </head>

  <body>
    {% block btop %}
    <a class="d-none btn btn-secondary" id="back-to-top" href="#"
      >Back to Top</a
    >
    {% endblock %} {% block main %}
    <div class="container" style="padding: 60px 15px">
      <div style="text-align: center">
        <h1>Lookit the stuff here!</h1>
        <p class="lead">Oops, nothing to see here. Come back later then!</p>
      </div>
    </div>
    <!-- /.container -->
    {% endblock %}

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script
      src="//{{ static_domain }}/static/vendor/js/bootstrap.bundle.min.js.br"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    ></script>
    <script
      src="//{{ static_domain }}/static/vendor/js/cookieconsent.min.js.br"
      integrity="sha256-5VhCqFam2Cn+yjw61zbBNrbHVJ6SRydPeKopYlngbiQ="
      crossorigin="anonymous"
    ></script>
    <script src="//{{ static_domain }}/static/js/base.js"></script>
    {% block tail %} {% endblock %} {% include "analytics.html.j2" %}
  </body>
</html>
